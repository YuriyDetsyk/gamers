{
  "name": "gamers-api",
  "version": "1.0.0",
  "description": "Interactive WebRTC Application for Gamers",
  "repository": {
    "type": "git",
    "url": "https://github.com/YuriyDetsyk/gamers"
  },
  "keywords": [
    "gamers",
    "gamersorg",
    "chunkup",
    "yuriydetsyk"
  ],
  "author": "yuriydetsyk",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node -r ts-node/register/transpile-only ./index.ts",
    "watch": "nodemon --inspect=9300 ./index.ts",
    "build": "tsc -p ../tsconfig.server.json",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "peer": "0.6.1",
    "twilio": "3.82.0",
    "express": "4.18.1",
    "sequelize": "6.23.1"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "sequelize-cli": "6.4.1"
  }
}
